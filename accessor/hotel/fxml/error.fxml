<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="accessor.hotel.fxml.ErrorController">
   <children>
      <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="500.0" styleClass="root">
         <children>
            <VBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label styleClass="text-label" text="Произошла системная ошибка&#10;Пожалуйста посмотрите журнал&#10;" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
                  <Label fx:id="journalNameLabel" styleClass="text-label" text="Номер журнала: " textAlignment="CENTER" VBox.vgrow="ALWAYS" />
                  <StackPane prefWidth="200.0">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                     <children>
                        <Button mnemonicParsing="false" onAction="#closeAction" text="Закрыть" />
                     </children>
                  </StackPane>
               </children>
            </VBox>
         </children>
         <effect>
            <DropShadow />
         </effect>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</StackPane>
